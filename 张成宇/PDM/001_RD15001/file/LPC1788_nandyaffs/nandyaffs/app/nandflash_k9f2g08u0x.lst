N/* --------------------- BIT DEFINITIONS -------------------------------------- */
N/**********************************************************************
N** Interrupt information
N**********************************************************************/
N/** Macro to clean interrupt pending */
N#define TIM_IR_CLR(n) _BIT(n)
N
N/**********************************************************************
N** Timer interrupt register definitions
N**********************************************************************/
N/** Macro for getting a timer match interrupt bit */
N#define TIM_MATCH_INT(n)		(_BIT(n & 0x0F))
N/** Macro for getting a capture event interrupt bit */
N#define TIM_CAP_INT(n)     (_BIT(((n & 0x0F) + 4)))
N
N/**********************************************************************
N* Timer control register definitions
N**********************************************************************/
N/** Timer/counter enable bit */
N#define TIM_ENABLE			((uint32_t)(1<<0))
N/** Timer/counter reset bit */
N#define TIM_RESET			((uint32_t)(1<<1))
N/** Timer control bit mask */
N#define TIM_TCR_MASKBIT		((uint32_t)(3))
N
N/**********************************************************************
N* Timer match control register definitions
N**********************************************************************/
N/** Bit location for interrupt on MRx match, n = 0 to 3 */
N#define TIM_INT_ON_MATCH(n)      	(_BIT((n * 3)))
N/** Bit location for reset on MRx match, n = 0 to 3 */
N#define TIM_RESET_ON_MATCH(n)    	(_BIT(((n * 3) + 1)))
N/** Bit location for stop on MRx match, n = 0 to 3 */
N#define TIM_STOP_ON_MATCH(n)     	(_BIT(((n * 3) + 2)))
N/** Timer Match control bit mask */
N#define TIM_MCR_MASKBIT			   ((uint32_t)(0x0FFF))
N/** Timer Match control bit mask for specific channel*/
N#define	TIM_MCR_CHANNEL_MASKBIT(n)		((uint32_t)(7<<(n*3)))
N
N/**********************************************************************
N* Timer capture control register definitions
N**********************************************************************/
N/** Bit location for CAP.n on CRx rising edge, n = 0 to 3 */
N#define TIM_CAP_RISING(n)   	(_BIT((n * 3)))
N/** Bit location for CAP.n on CRx falling edge, n = 0 to 3 */
N#define TIM_CAP_FALLING(n)   	(_BIT(((n * 3) + 1)))
N/** Bit location for CAP.n on CRx interrupt enable, n = 0 to 3 */
N#define TIM_INT_ON_CAP(n)    	(_BIT(((n * 3) + 2)))
N/** Mask bit for rising and falling edge bit */
N#define TIM_EDGE_MASK(n)		(_SBF((n * 3), 0x03))
N/** Timer capture control bit mask */
N#define TIM_CCR_MASKBIT			((uint32_t)(0x3F))
N/** Timer Capture control bit mask for specific channel*/
N#define	TIM_CCR_CHANNEL_MASKBIT(n)		((uint32_t)(7<<(n*3)))
N
N/**********************************************************************
N* Timer external match register definitions
N**********************************************************************/
N/** Bit location for output state change of MAT.n when external match
N   happens, n = 0 to 3 */
N#define TIM_EM(n)    			_BIT(n)
N/** Output state change of MAT.n when external match happens: no change */
N#define TIM_EM_NOTHING    	((uint8_t)(0x0))
N/** Output state change of MAT.n when external match happens: low */
N#define TIM_EM_LOW         	((uint8_t)(0x1))
N/** Output state change of MAT.n when external match happens: high */
N#define TIM_EM_HIGH        	((uint8_t)(0x2))
N/** Output state change of MAT.n when external match happens: toggle */
N#define TIM_EM_TOGGLE      	((uint8_t)(0x3))
N/** Macro for setting for the MAT.n change state bits */
N#define TIM_EM_SET(n,s) 	(_SBF(((n << 1) + 4), (s & 0x03)))
N/** Mask for the MAT.n change state bits */
N#define TIM_EM_MASK(n) 		(_SBF(((n << 1) + 4), 0x03))
N/** Timer external match bit mask */
N#define TIM_EMR_MASKBIT	0x0FFF
N
N/**********************************************************************
N* Timer Count Control Register definitions
N**********************************************************************/
N/** Mask to get the Counter/timer mode bits */
N#define TIM_CTCR_MODE_MASK  0x3
N/** Mask to get the count input select bits */
N#define TIM_CTCR_INPUT_MASK 0xC
N/** Timer Count control bit mask */
N#define TIM_CTCR_MASKBIT	0xF
N#define TIM_COUNTER_MODE ((uint8_t)(1))
N

N/*********************************************************************//**
N * @brief 		Read status from NAND FLASH memory
N * @param[in]	Cmd	command for read operation, should be:
N * 					-  K9FXX_BLOCK_PROGRAM_1
N *					-  K9FXX_BLOCK_ERASE_1
N *					-  K9FXX_READ_1
N * @return 		Status, could be:
N *				- TRUE: pass
N *				- FALSE: Failure
N **********************************************************************/
NBool NandFlash_ReadStatus(uint32_t Cmd)
N{
N	volatile uint8_t *pCLE;
N	volatile uint8_t *pDATA;
N	uint8_t StatusData;
N
N	pCLE  = K9F1G_CLE;
X	pCLE  = ((volatile uint8_t *)(0x80000000|(0x1<<20)));
N	pDATA = K9F1G_DATA;
X	pDATA = ((volatile uint8_t *)0x81000000);
N
N	*pCLE = K9FXX_READ_STATUS;
X	*pCLE = 0x70;
N
N	while ( (*pDATA & 0xC0) != 0xC0 );
N
N	StatusData = *pDATA;
N
N	switch (Cmd)
N	{
N		case K9FXX_BLOCK_PROGRAM_1:
X		case 0x80:
N		case K9FXX_BLOCK_ERASE_1:
X		case 0x60:
N		  	if (StatusData & 0x01)	/* Erase/Program failure(1) or pass(0) */
N				return(FALSE);
N		  	else
N				return(TRUE);
N
N		case K9FXX_READ_1:				/* bit 5 and 6, Read busy(0) or ready(1) */
X		case 0x00:				 
N		  	return(TRUE);
N
N		default:
N		  	break;
N	}
N
N	return(FALSE);
N}
